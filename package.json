{
  "name": "janim-toolbox",
  "displayName": "janim-toolbox",
  "description": "",
  "publisher": "jkjkil4",
  "icon": "icon.png",
  "version": "1.4.1",
  "repository": "https://github.com/jkjkil4/janim-toolbox",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "janim-toolbox.connect",
        "title": "JAnim: Connect to JAnim Window"
      },
      {
        "command": "janim-toolbox.reload",
        "title": "JAnim: Reload Current File"
      }
    ],
    "keybindings": [
      {
        "command": "janim-toolbox.connect",
        "key": "ctrl+J ctrl+C"
      },
      {
        "command": "janim-toolbox.reload",
        "key": "ctrl+J ctrl+R"
      }
    ],
    "colors": [
      {
        "id": "janim_toolbox.lineno_background",
        "description": "Background decoration color for line hint",
        "defaults": {
          "dark": "#FF000055",
          "light": "#FF000055",
          "highContrast": "#FF000055"
        }
      }
    ],
    "configuration": {
      "title": "janim-toolbox Settings",
      "properties": {
        "janim-toolbox.clientSearchPort": {
          "type": "number",
          "default": 40565,
          "description": "端口，用于搜索可用界面端",
          "minimum": 1,
          "maximum": 65535
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@vscode/vsce": "^3.2.2"
  }
}
