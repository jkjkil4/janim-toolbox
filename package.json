{
  "name": "janim-toolbox",
  "displayName": "janim-toolbox",
  "description": "",
  "publisher": "jkjkil4",
  "version": "0.2.0",
  "repository": "https://github.com/jkjkil4/janim-toolbox",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "janim-toolbox.reset",
        "title": "JAnim: Reset"
      },
      {
        "command": "janim-toolbox.execute-code",
        "title": "JAnim: Execute Selection Code"
      },
      {
        "command": "janim-toolbox.undo-code",
        "title": "JAnim: Undo Code"
      },
      {
        "command": "janim-toolbox.display-children-index",
        "title": "JAnim: Display Children Index"
      }
    ],
    "keybindings": [
      {
        "command": "janim-toolbox.reset",
        "key": "ctrl+J ctrl+R"
      },
      {
        "command": "janim-toolbox.execute-code",
        "key": "ctrl+J ctrl+C"
      },
      {
        "command": "janim-toolbox.undo-code",
        "key": "ctrl+J ctrl+U"
      },
      {
        "command": "janim-toolbox.display-children-index",
        "key": "ctrl+J ctrl+I"
      }
    ],
    "colors": [
      {
				"id": "janim_toolbox.hint_background",
				"description": "Background decoration color for line hint",
				"defaults": {
					"dark": "#FF000055",
					"light": "#FF000055",
					"highContrast": "#FF000055"
				}
			}
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
